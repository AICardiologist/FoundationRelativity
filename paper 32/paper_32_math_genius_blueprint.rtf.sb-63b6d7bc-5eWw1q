\f1\b \cf0 1. Defs.lean (Infrastructure)\
\pard\pardeftab720\sa240\partightenfactor0

\f2\i\b0 \cf0 Axiom profile: [] (Pure BISH - using Mathlib's Real infrastructure)
\f0\i0 \
\pard\pardeftab720\partightenfactor0
\cf0 Lean\
\
import Mathlib.Data.Real.Basic\
import Mathlib.Analysis.SpecialFunctions.Exp\
import Mathlib.Analysis.SpecialFunctions.Log.Basic\
import Mathlib.Topology.Instances.Real\
\
noncomputable section \
\
def b_coeff : \uc0\u8477  := 2 / (3 * Real.pi)\
\
def discrete_rg_step (\uc0\u945 _n \u948  : \u8477 ) : \u8477  :=\
  \uc0\u945 _n + b_coeff * (\u945 _n ^ 2) * \u948 \
\
def alpha_exact (\uc0\u945 \u8320  \u956 \u8320  \u956  : \u8477 ) : \u8477  :=\
  \uc0\u945 \u8320  / (1 - b_coeff * \u945 \u8320  * Real.log (\u956  / \u956 \u8320 ))\
\
def mu_L (\uc0\u945 \u8320  \u956 \u8320  : \u8477 ) : \u8477  := \
  \uc0\u956 \u8320  * Real.exp (1 / (b_coeff * \u945 \u8320 ))\
